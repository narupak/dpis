pipeline {
	agent any
	stages {
		stage('Init'){
			steps {
				echo 'init'
				echo '****************************'
			}
		}
		stage('Yarn install'){
			steps {
				echo 'Yarn install'
				echo '****************************'
			}
		}
		stage('Yarn build'){
			steps {
				echo 'Yarn build'
				echo '****************************'
			}
		}
		stage('Deploy'){
			steps {
				echo 'Deploy'
				echo '****************************'
			}
		}
	}
}