pipeline {
	agent any
	stages {
		state('Init'){
			steps {
				echo 'init'
				echo '****************************'
			}
		}
		state('Yarn install'){
			steps {
				echo 'Yarn install'
				echo '****************************'
			}
		}
		state('Yarn build'){
			steps {
				echo 'Yarn build'
				echo '****************************'
			}
		}
		state('Deploy'){
			steps {
				echo 'Deploy'
				echo '****************************'
			}
		}
	}
}